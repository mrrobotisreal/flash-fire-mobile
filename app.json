{
  "expo": {
    "name": "Flash-Fire mobile",
    "slug": "Flash-Fire-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/Flash-Fire.gif",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iamwintrow.flash_fire"
    },
    "android": {
      "package": "com.iamwintrow.flash_fire",
      "adaptiveIcon": {
        "foregroundImage": "./assets/Flash-Fire.gif",
        "backgroundColor": "#000000"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
